{
  "ui_settings": {
    "theme": "light",
    "language": "en-US",
    "page_size": 20,
    "show_debug": false,
    "auto_refresh": false
  },
  "llm_settings": {
    "default_provider": "google",
    "enabled_providers": [
      "google",
      "volcano"
    ],
    "enable_fallback": true,
    "fallback_order": [
      "google",
      "volcano"
    ]
  },
  "google_settings": {
    "model": "gemini-1.5-pro",
    "temperature": 0.7,
    "max_tokens": 4000,
    "timeout": 30,
    "retries": 3
  },
  "volcano_settings": {
    "base_url": "https://ark.cn-beijing.volces.com/api/v3",
    "model": "doubao-1.5-pro-32k",
    "temperature": 0.7
  },
  "multimodal_settings": {
    "enabled": true,
    "max_image_size": 10,
    "image_timeout": 60,
    "supported_formats": [
      "jpg",
      "jpeg",
      "png"
    ]
  },
  "data_processing": {
    "max_file_size": 100,
    "chunk_size": 10000,
    "memory_limit": 4,
    "temp_dir": "data/temp",
    "auto_cleanup": true
  }
}